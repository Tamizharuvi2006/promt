{
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://promptweb.app/$1",
      "permanent": true,
      "has": [{ "type": "host", "value": "www.promptweb.app" }]
    }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
